{
  // Intellisense Specific Config
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": false,
    "declaration": false,
    // "emitDecoratorMetadata": false,
    // "esModuleInterop": false,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "incremental": true,
    // "inlineSourceMap": true,
    "lib": ["ESNext"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmit": true,
    "noEmitHelpers": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "rootDir": ".",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceRoot": "/",
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": false,
    "target": "ESNext",
    // "types": ["node"],

    "baseUrl": ".",
    "paths": {
      "@omedym/*": ["dist/bin/node_modules/@omedym/*"],
      "@omedym/nestjs-dmq-core": ["./packages/core/src/index"],
      "@omedym/nestjs-dmq-repository-postgres": ["./packages/repository-postgres/src/index"],
    },
    // "compileOnSave": false,
  },
}
